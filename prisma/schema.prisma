datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model Parameter {
  id    Int    @id() @default(autoincrement())
  /// @length(1, 256)
  name  String @unique()
  value String
  /// @length(1, 256)
  type  String
}

model Version {
  id    Int @id() @default(autoincrement())
  value Int @default(0)
}
